                                            roomx = roomx - 3;
                                        }
                                        if(roomx < -350){
                                            roomx = roomx + 3;
                                        }
                                        if(roomy === 30){
                                            roomy = roomy - 3;
                                        }
                                        if(roomy < -150){
                                            roomy = roomy + 3;
                                        }
                                        fill(0, 0, 255);
                                        text("press s to stop", 20, 50);
                                    };
                                }
                            };
                        }    
                    };
                }
            };
        };    
    }
};
